{
  "name": "p5.dumblivereload",
  "version": "0.0.3-0",
  "description": "Live reload p5 sketches without reloading the whole page",
  "source": "src/index.js",
  "p5dlr.min": "dist/p5dumbLiveReload.min.js",
  "p5dlr": "dist/p5dumbLiveReload.js",
  "targets": {
    "p5dlr.min": {
      "optimize": true,
      "isLibrary": false,
      "outputFormat": "global"
    },
    "p5dlr": {
      "optimize": false,
      "isLibrary": false,
      "outputFormat": "global"
    }
  },
  "scripts": {
    "watch": "parcel watch",
    "build": "parcel build --no-source-maps"
  },
  "keywords": [
    "p5.js",
    "live",
    "reload",
    "dumb",
    "live",
    "reload",
    "live",
    "coding"
  ],
  "author": "Tibor Udvari",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/TiborUdvari/p5.dumbLiveReload.git"
  },
  "homepage": "https://github.com/TiborUdvari/p5.dumbLiveReload#readme",
  "devDependencies": {
    "parcel": "^2.12.0"
  }
}
